    <style>

        :root {
            --primary-color: #2563eb;
            --secondary-color: #1e40af;
            --accent-color: #06b6d4;
            --background-color: #0f172a;
            --techzen-accent: #10b981;
            --card-color: rgba(255, 255, 255, 0.1);
            --text-color: #e2e8f0;
            --navbar-height: 4rem;
            --gradient-primary: linear-gradient(135deg, var(--primary-color), var(--secondary-color));
            --gradient-techzen: linear-gradient(135deg, var(--techzen-accent), var(--accent-color));
            --card-hover-shadow: 0 10px 30px rgba(16, 185, 129, 0.2);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }



        /* Navbar Styles */
        .navbar {
            position: fixed;
            top: 0;
            width: 100%;
            height: var(--navbar-height);
            background: rgba(15, 23, 42, 0.8);
            backdrop-filter: blur(10px);
            z-index: 100;
            padding: 0 2rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
            border-bottom: 1px solid rgba(16, 185, 129, 0.2);
        }

        .nav-logo {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            color: var(--text-color);
            font-size: 1.5rem;
            font-weight: bold;
            text-decoration: none;
        }

        .logo-highlight {
            color: var(--techzen-accent);
        }

        .nav-links {
            display: flex;
            gap: 2rem;
            align-items: center;
        }

        .nav-link {
            color: var(--text-color);
            text-decoration: none;
            transition: all 0.3s;
            display: flex;
            align-items: center;
            gap: 0.25rem;
            position: relative;
        }

        .nav-link:hover {
            color: var(--techzen-accent);
        }

        .nav-link::after {
            content: '';
            position: absolute;
            bottom: -5px;
            left: 0;
            width: 0;
            height: 2px;
            background: var(--techzen-accent);
            transition: width 0.3s;
        }

        .nav-link:hover::after {
            width: 100%;
        }

        .techzen-button {
            background: var(--gradient-techzen);
            color: white;
            padding: 0.5rem 1rem;
            border-radius: 20px;
            font-weight: 500;
            border: none;
            cursor: pointer;
            transition: all 0.3s;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .techzen-button:hover {
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.3);
        }

        .mobile-menu-btn {
            display: none;
            background: none;
            border: none;
            color: var(--text-color);
            font-size: 1.5rem;
            cursor: pointer;
        }

        /* Main Content Styles */
        body {
            background-color: var(--background-color);
            color: var(--text-color);
            padding-top: var(--navbar-height);
        }

/* Replace the header style with this updated version */
.header {
    position: relative;
    padding: 6rem 2rem 4rem;
    text-align: center;
    overflow: hidden;
    color: var(--text-color);
}

.header::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(15, 23, 42, 0.7); /* Dark overlay for better text visibility */
    z-index: 1;
}

.header-video {
    position: absolute;
    top: 50%;
    left: 50%;
    min-width: 100%;
    min-height: 100%;
    width: auto;
    height: auto;
    transform: translateX(-50%) translateY(-50%);
    z-index: 0;
    object-fit: cover;
}

.header h1, 
.header p, 
.header .techzen-badge, 
.header .search-container {
    position: relative;
    z-index: 2; /* Ensure text is above the video */
}

.header h1 {
    font-size: 3.5rem;
    margin-bottom: 1rem;
    text-shadow: 0 0 20px rgba(0, 0, 0, 0.5);
    background: -webkit-linear-gradient(#ffffff, #10b981);
    -webkit-background-clip: text;
    background-clip: text;
    -webkit-text-fill-color: transparent;
    font-weight: 800;
}

        .techzen-badge {
            display: inline-flex;
            align-items: center;
            background: rgba(16, 185, 129, 0.2);
            border: 1px solid var(--techzen-accent);
            color: var(--techzen-accent);
            padding: 0.5rem 1rem;
            border-radius: 20px;
            font-size: 0.9rem;
            margin-bottom: 1.5rem;
            gap: 0.5rem;
        }

        .search-container {
            max-width: 700px;
            margin: 2rem auto;
            padding: 0 1rem;
            position: relative;
        }

        .search-bar {
            width: 100%;
            padding: 1rem 2.5rem;
            border: none;
            border-radius: 30px;
            font-size: 1rem;
            background: rgba(255, 255, 255, 0.1);
            color: var(--text-color);
            backdrop-filter: blur(10px);
            transition: all 0.3s;
            border: 1px solid rgba(16, 185, 129, 0.2);
        }

        .search-bar:focus {
            outline: none;
            background: rgba(255, 255, 255, 0.15);
            box-shadow: 0 0 20px rgba(16, 185, 129, 0.3);
            border-color: var(--techzen-accent);
        }

        .search-icon {
    position: absolute;
    top: 50%;
    left: -10px;
    margin-top: -20px;
    transform: translateY(-50%);
    color: var(--techzen-accent);
    pointer-events: none;
} 

        .filter-options {
            display: flex;
            justify-content: center;
            gap: 1rem;
            margin-top: 1rem;
            flex-wrap: wrap;
        }

        .filter-btn {
            background: rgba(255, 255, 255, 0.1);
            border: 1px solid rgba(16, 185, 129, 0.2);
            color: var(--text-color);
            padding: 0.5rem 1rem;
            border-radius: 20px;
            cursor: pointer;
            transition: all 0.3s;
        }

        .filter-btn:hover, .filter-btn.active {
            background: rgba(16, 185, 129, 0.2);
            border-color: var(--techzen-accent);
            color: var(--techzen-accent);
        }

        .container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 2rem 1rem;
        }

        .section-title {
            font-size: 2rem;
            margin-bottom: 2rem;
            text-align: center;
            color: var(--accent-color);
            position: relative;
            display: inline-block;
            left: 50%;
            transform: translateX(-50%);
        }

        .section-title::after {
            content: '';
            position: absolute;
            bottom: -10px;
            left: 50%;
            transform: translateX(-50%);
            width: 50px;
            height: 3px;
            background: var(--techzen-accent);
        }

        .launches-grid {
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(350px, 1fr));
            gap: 2rem;
            margin-top: 2rem;
        }

        .launch-card {
            background: var(--card-color);
            border-radius: 15px;
            padding: 1.5rem;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.1);
            transition: all 0.3s;
            display: flex;
            flex-direction: column;
            height: 100%;
        }

        .launch-card:hover {
            transform: translateY(-10px);
            box-shadow: var(--card-hover-shadow);
            border-color: rgba(16, 185, 129, 0.3);
        }

        .launch-card h3 {
            color: var(--accent-color);
            font-size: 1.5rem;
            margin-bottom: 1rem;
        }

        .launch-info {
            color: #94a3b8;
            font-size: 0.9rem;
            margin-bottom: 1rem;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .company-badge {
            background: rgba(16, 185, 129, 0.2);
            color: var(--techzen-accent);
            padding: 0.25rem 0.75rem;
            border-radius: 15px;
            font-size: 0.8rem;
        }

        .tags {
            display: flex;
            flex-wrap: wrap;
            gap: 0.5rem;
            margin-top: 1rem;
        }

        .tag {
            background: rgba(37, 99, 235, 0.2);
            color: var(--text-color);
            padding: 0.25rem 0.75rem;
            border-radius: 15px;
            font-size: 0.8rem;
            transition: background-color 0.3s;
        }

        .tag:hover {
            background: rgba(37, 99, 235, 0.3);
        }

        .action-buttons {
            margin-top: auto;
            padding-top: 1.5rem;
            display: flex;
            gap: 1rem;
        }

        .visit-button {
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
            background: var(--gradient-primary);
            color: white;
            padding: 0.75rem 1.5rem;
            border-radius: 8px;
            text-decoration: none;
            transition: all 0.3s;
            flex: 1;
            justify-content: center;
        }

        .compare-button {
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
            background: rgba(16, 185, 129, 0.1);
            color: var(--techzen-accent);
            border: 1px solid var(--techzen-accent);
            padding: 0.75rem 1.5rem;
            border-radius: 8px;
            text-decoration: none;
            transition: all 0.3s;
            flex: 1;
            justify-content: center;
        }

        .visit-button:hover, .compare-button:hover {
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.3);
        }

        .compare-button:hover {
            background: rgba(16, 185, 129, 0.2);
        }

        /* Trending Section */
        .trending-section {
            margin-top: 4rem;
            padding: 2rem;
            background: rgba(255, 255, 255, 0.05);
            border-radius: 20px;
            border: 1px solid rgba(16, 185, 129, 0.2);
        }

        .trend-item {
            display: flex;
            align-items: center;
            gap: 1rem;
            margin-bottom: 1rem;
            padding: 1rem;
            background: rgba(255, 255, 255, 0.05);
            border-radius: 10px;
            transition: transform 0.3s;
            border: 1px solid transparent;
        }

        .trend-item:hover {
            transform: scale(1.02);
            border-color: rgba(16, 185, 129, 0.3);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.1);
        }

        .trend-icon {
            background: var(--gradient-techzen);
            width: 50px;
            height: 50px;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 1.5rem;
        }

        .trend-content {
            flex: 1;
        }

        .trend-badge {
            background: rgba(16, 185, 129, 0.2);
            color: var(--techzen-accent);
            padding: 0.25rem 0.75rem;
            border-radius: 15px;
            font-size: 0.8rem;
            display: inline-flex;
            align-items: center;
            gap: 0.25rem;
            margin-top: 0.5rem;
        }

        .trend-up {
            color: var(--techzen-accent);
        }

        .trend-stable {
            color: var(--text-color);
        }

        /* Newsletter Section */
        .newsletter-section {
            margin-top: 4rem;
            padding: 3rem 2rem;
            background: var(--gradient-primary);
            border-radius: 20px;
            text-align: center;
            position: relative;
            overflow: hidden;
        }

        .newsletter-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: url('/api/placeholder/1200/400') center/cover;
            opacity: 0.1;
        }

        .newsletter-form {
            max-width: 500px;
            margin: 2rem auto 0;
            display: flex;
            gap: 1rem;
            position: relative;
        }

        .newsletter-input {
            flex: 1;
            padding: 1rem 1.5rem;
            border: none;
            border-radius: 30px;
            font-size: 1rem;
            background: rgba(255, 255, 255, 0.2);
            color: white;
            backdrop-filter: blur(10px);
        }

        .newsletter-input::placeholder {
            color: rgba(255, 255, 255, 0.7);
        }

        .newsletter-input:focus {
            outline: none;
            background: rgba(255, 255, 255, 0.3);
        }

        .newsletter-button {
            background: var(--techzen-accent);
            color: white;
            padding: 1rem 2rem;
            border: none;
            border-radius: 30px;
            font-weight: bold;
            cursor: pointer;
            transition: all 0.3s;
            white-space: nowrap;
        }

        .newsletter-button:hover {
            background: #0da271;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.3);
        }


        .ai-analytics-section {
            padding: 4rem 2rem;
            background: rgba(15, 23, 42, 0.6);
            border-radius: 20px;
            margin-top: 4rem;
        }

        .analytics-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 2rem;
            margin-top: 2rem;
        }

        .analytics-card {
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            padding: 1.5rem;
            border: 1px solid rgba(16, 185, 129, 0.2);
            transition: all 0.3s;
        }

        .analytics-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 30px rgba(16, 185, 129, 0.2);
            border-color: rgba(16, 185, 129, 0.4);
        }

        .analytics-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1.5rem;
        }

        .refresh-btn {
            cursor: pointer;
            width: 35px;
            height: 35px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 50%;
            background: rgba(16, 185, 129, 0.1);
            transition: all 0.3s;
        }

        .refresh-btn:hover {
            background: rgba(16, 185, 129, 0.2);
            transform: rotate(180deg);
        }

        .metrics-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 1rem;
            margin-top: 1.5rem;
        }

        .metric {
            text-align: center;
            padding: 1rem;
            background: rgba(255, 255, 255, 0.03);
            border-radius: 10px;
            border: 1px solid rgba(16, 185, 129, 0.1);
        }

        .metric-value {
            display: block;
            font-size: 1.5rem;
            font-weight: bold;
            color: var(--techzen-accent);
        }

        .metric-label {
            font-size: 0.9rem;
            color: var(--text-color);
            opacity: 0.8;
        }

        .comparison-controls {
            display: flex;
            align-items: center;
            gap: 1rem;
            margin-bottom: 1.5rem;
        }

        .model-select {
            flex: 1;
            padding: 0.75rem;
            border-radius: 8px;
            background: rgba(255, 255, 255, 0.05);
            border: 1px solid rgba(16, 185, 129, 0.2);
            color: var(--text-color);
            cursor: pointer;
        }

        .benchmark-tabs {
            display: flex;
            gap: 0.5rem;
            margin-bottom: 1.5rem;
        }

        .tab-btn {
            padding: 0.5rem 1rem;
            border: none;
            background: rgba(255, 255, 255, 0.05);
            color: var(--text-color);
            border-radius: 6px;
            cursor: pointer;
            transition: all 0.3s;
        }

        .tab-btn.active {
            background: var(--techzen-accent);
            color: white;
        }

        .performance-chart, .comparison-chart {
            height: 200px;
            margin: 1rem 0;
        }

        /* Footer Styles */
        .footer {
            background: rgba(15, 23, 42, 0.9);
            padding: 4rem 2rem;
            margin-top: 4rem;
            border-top: 1px solid rgba(16, 185, 129, 0.2);
        }

        .footer-content {
            max-width: 1200px;
            margin: 0 auto;
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 2rem;
        }

        .footer-section h4 {
            color: var(--techzen-accent);
            margin-bottom: 1rem;
            position: relative;
            display: inline-block;
        }

        .footer-section h4::after {
            content: '';
            position: absolute;
            bottom: -5px;
            left: 0;
            width: 30px;
            height: 2px;
            background: var(--techzen-accent);
        }

        .footer-section ul {
            list-style: none;
        }

        .footer-section ul li {
            margin-bottom: 0.75rem;
        }

        .footer-section a {
            color: var(--text-color);
            text-decoration: none;
            transition: color 0.3s;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .footer-section a:hover {
            color: var(--techzen-accent);
        }

        .footer-bottom {
            max-width: 1200px;
            margin: 3rem auto 0;
            padding-top: 2rem;
            border-top: 1px solid rgba(255, 255, 255, 0.1);
            display: flex;
            justify-content: space-between;
            align-items: center;
            flex-wrap: wrap;
            gap: 1rem;
        }

        .footer-logo {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            color: var(--text-color);
            font-size: 1.25rem;
            font-weight: bold;
        }

        .social-links {
            display: flex;
            gap: 1rem;
        }

        .social-link {
            width: 40px;
            height: 40px;
            background: rgba(255, 255, 255, 0.1);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            color: var(--text-color);
            transition: all 0.3s;
            font-size: 1.25rem;
        }

        .social-link:hover {
            background: var(--techzen-accent);
            color: white;
            transform: translateY(-3px);
        }

        /* Animations */
        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        @keyframes pulse {
            0% { opacity: 0.5; }
            50% { opacity: 1; }
            100% { opacity: 0.5; }
        }

        @keyframes parallax {
            0% { transform: translateY(0); }
            50% { transform: translateY(-10px); }
            100% { transform: translateY(0); }
        }

        .no-results {
            text-align: center;
            padding: 3rem;
            background: rgba(255, 255, 255, 0.05);
            border-radius: 15px;
            margin-top: 2rem;
        }

        .blink {
            animation: blink 1.5s infinite;
        }

        @keyframes blink {
            0%, 100% { opacity: 1; }
            50% { opacity: 0.3; }
        }

        /* Mobile Menu */
        .mobile-menu {
            position: fixed;
            top: var(--navbar-height);
            left: 0;
            width: 100%;
            background: rgba(15, 23, 42, 0.95);
            backdrop-filter: blur(10px);
            padding: 1rem;
            z-index: 99;
            transform: translateY(-100%);
            transition: transform 0.3s;
            border-bottom: 1px solid rgba(16, 185, 129, 0.2);
        }

        .mobile-menu.open {
            transform: translateY(0);
        }

        .mobile-menu a {
            display: block;
            padding: 0.75rem 1rem;
            color: var(--text-color);
            text-decoration: none;
            border-bottom: 1px solid rgba(255, 255, 255, 0.1);
            transition: all 0.3s;
        }

        .mobile-menu a:last-child {
            border-bottom: none;
        }

        .mobile-menu a:hover {
            background: rgba(16, 185, 129, 0.1);
            color: var(--techzen-accent);
        }

        .back-to-top {
            position: fixed;
            bottom: 2rem;
            right: 2rem;
            width: 50px;
            height: 50px;
            background: var(--gradient-techzen);
            color: white;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.25rem;
            cursor: pointer;
            transition: all 0.3s;
            opacity: 0;
            visibility: hidden;
            z-index: 98;
        }

        .back-to-top.visible {
            opacity: 1;
            visibility: visible;
        }

        .back-to-top:hover {
            transform: translateY(-5px);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.3);
        }

        /* Responsive Design */
        @media (max-width: 992px) {
            .header h1 {
                font-size: 2.5rem;
            }
        }

        @media (max-width: 768px) {
            .header h1 {
                font-size: 2rem;
            }
            
            .launches-grid {
                grid-template-columns: 1fr;
            }

            .nav-links {
                display: none;
            }

            .mobile-menu-btn {
                display: block;
            }

            .newsletter-form {
                flex-direction: column;
            }

            .newsletter-button {
                width: 100%;
            }
        }

        @media (max-width: 480px) {
            .header h1 {
                font-size: 1.75rem;
            }

            .header p {
                font-size: 1rem;
            }

            .search-container {
                margin: 1rem auto;
            }

            .action-buttons {
                flex-direction: column;
            }

            .newsletter-section {
                padding: 2rem 1rem;
            }
        }


