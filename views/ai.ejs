/* Tech background styles */
#background {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: var(--background-color);
    z-index: -1;
    overflow: hidden;
    background-image: 
        radial-gradient(circle at 25% 25%, rgba(16, 185, 129, 0.05) 2%, transparent 12%),
        radial-gradient(circle at 75% 44%, rgba(6, 182, 212, 0.05) 2%, transparent 10%),
        radial-gradient(circle at 35% 65%, rgba(37, 99, 235, 0.05) 1%, transparent 8%),
        linear-gradient(to bottom, rgba(15, 23, 42, 0.7), rgba(15, 23, 42, 0.9));
    background-size: 50px 50px, 70px 70px, 60px 60px, 100% 100%;
}

.tech-grid {
    position: absolute;
    width: 100%;
    height: 100%;
    background-image: 
        linear-gradient(to right, rgba(16, 185, 129, 0.05) 1px, transparent 1px),
        linear-gradient(to bottom, rgba(16, 185, 129, 0.05) 1px, transparent 1px);
    background-size: 40px 40px;
}

.tech-circuit {
    position: absolute;
    width: 100%;
