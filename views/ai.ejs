    <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>
    <script>
        // Initialize AOS (Animate On Scroll)
        document.addEventListener('DOMContentLoaded', function() {
            AOS.init({
                duration: 800,
                easing: 'ease-in-out',
                once: true
            });
            
            // Loader Animation
            setTimeout(function() {
                const loader = document.getElementById('loader');
                loader.style.opacity = '0';
                setTimeout(function() {
                    loader.style.display = 'none';
                }, 500);
            }, 2000);
            
            // Create loader particles
            const particlesContainer = document.getElementById('particles');
            for (let i = 0; i < 20; i++) {
                const particle = document.createElement('div');
                particle.classList.add('particle');
                
                // Random position and animation
                const angle = Math.random() * Math.PI * 2;
                const distance = 50 + Math.random() * 100;
                const tx = Math.cos(angle) * distance;
                const ty = Math.sin(angle) * distance;
                
                particle.style.setProperty('--tx', `${tx}px`);
                particle.style.setProperty('--ty', `${ty}px`);
                particle.style.left = '50%';
                particle.style.top = '50%';
                particle.style.animationDelay = `${Math.random()}s`;
                particle.style.animationDuration = `${1 + Math.random()}s`;
                
                particlesContainer.appendChild(particle);
            }
            
            // Mobile Menu Toggle
            const mobileMenuBtn = document.getElementById('mobile-menu-toggle');
            const mobileMenu = document.getElementById('mobile-menu');
            
            mobileMenuBtn.addEventListener('click', function() {
                mobileMenu.classList.toggle('open');
            });
            
            // Search Functionality
            const searchInput = document.getElementById('search-input');
            const launchCards = document.querySelectorAll('.launch-card');
            const noResults = document.getElementById('no-results');
            const filterBtns = document.querySelectorAll('.filter-btn');
            
            function filterLaunches() {
                const searchTerm = searchInput.value.toLowerCase();
                const activeFilter = document.querySelector('.filter-btn.active').getAttribute('data-filter');
                
                let hasResults = false;
                
                launchCards.forEach(card => {
                    const cardTitle = card.querySelector('h3').textContent.toLowerCase();
                    const cardDesc = card.querySelector('p').textContent.toLowerCase();
                    const cardTags = card.getAttribute('data-tags');
                    
                    const matchesSearch = cardTitle.includes(searchTerm) || cardDesc.includes(searchTerm);
                    const matchesFilter = activeFilter === 'all' || cardTags.includes(activeFilter);
                    
                    if (matchesSearch && matchesFilter) {
                        card.style.display = 'flex';
                        hasResults = true;
                    } else {
                        card.style.display = 'none';
                    }
                });
                
                noResults.style.display = hasResults ? 'none' : 'block';
            }
            
            searchInput.addEventListener('input', filterLaunches);
            
            filterBtns.forEach(btn => {
                btn.addEventListener('click', function() {
                    filterBtns.forEach(b => b.classList.remove('active'));
                    this.classList.add('active');
                    filterLaunches();
                });
            });
            
            // Back to Top Button
            const backToTopBtn = document.getElementById('back-to-top');
            
            window.addEventListener('scroll', function() {
                if (window.scrollY > 300) {
                    backToTopBtn.classList.add('visible');
                } else {
                    backToTopBtn.classList.remove('visible');
                }
            });
            
            backToTopBtn.addEventListener('click', function() {
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            });
            
            // Newsletter Form Submission
            const newsletterForm = document.querySelector('.newsletter-form');
            
            newsletterForm.addEventListener('submit', function(e) {
                e.preventDefault();
                const emailInput = this.querySelector('input[type="email"]');
                
                // Simple validation
                if (emailInput.value) {
                    // Here you would typically send the data to your server
                    alert('Thanks for subscribing! You will receive the latest AI launch updates.');
                    emailInput.value = '';
                }
            });
        });


        document.addEventListener('DOMContentLoaded', function() {
            // Performance Chart
            const perfCtx = document.getElementById('performanceChart').getContext('2d');
            new Chart(perfCtx, {
                type: 'line',
                data: {
                    labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun'],
                    datasets: [{
                        label: 'Performance Score',
                        data: [95, 96, 97, 98, 99, 99.4],
                        borderColor: '#10b981',
                        tension: 0.4
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    }
                }
            });

            // Comparison Chart
            const compCtx = document.getElementById('comparisonChart').getContext('2d');
            new Chart(compCtx, {
                type: 'radar',
                data: {
                    labels: ['Reasoning', 'Vision', 'Language', 'Code', 'Speed', 'Accuracy'],
                    datasets: [{
                        label: 'Model 1',
                        data: [95, 88, 92, 85, 90, 94],
                        borderColor: '#10b981',
                        backgroundColor: 'rgba(16, 185, 129, 0.2)'
                    }, {
                        label: 'Model 2',
                        data: [88, 95, 90, 92, 85, 90],
                        borderColor: '#3b82f6',
                        backgroundColor: 'rgba(59, 130, 246, 0.2)'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false
                }
            });

            // Initialize benchmark grid
            const benchmarkData = {
                reasoning: [
                    { model: 'GPT-5 Omni', score: 98.5 },
                    { model: 'Claude 3.7', score: 97.2 },
                    { model: 'PaLM 3', score: 96.8 }
                ]
            };

            function updateBenchmarks(category) {
                const grid = document.getElementById('benchmarkGrid');
                grid.innerHTML = benchmarkData[category]
                    .map(item => `
                        <div class="benchmark-item">
                            <span class="model-name">${item.model}</span>
                            <div class="score-bar">
                                <div class="score-fill" style="width: ${item.score}%"></div>
                            </div>
                            <span class="score-value">${item.score}%</span>
                        </div>
                    `).join('');
            }

            updateBenchmarks('reasoning');

            // Tab switching
            document.querySelectorAll('.tab-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('active'));
                    btn.classList.add('active');
                    updateBenchmarks(btn.dataset.tab);
                });
            });
        });








// Create animated tech background
document.addEventListener('DOMContentLoaded', function() {
    const background = document.getElementById('background');
    
    // Add tech grid layer
    const techGrid = document.createElement('div');
    techGrid.className = 'tech-grid';
    background.appendChild(techGrid);
    
    // Add circuit pattern
    const techCircuit = document.createElement('div');
    techCircuit.className = 'tech-circuit';
    background.appendChild(techCircuit);
    
    // Create floating particles
    for (let i = 0; i < 20; i++) {
        createFloatingParticle(background);
    }
    
    // Create data streams
    for (let i = 0; i < 15; i++) {
        createDataStream(background);
    }
    
    // Create tech nodes
    for (let i = 0; i < 8; i++) {
        createTechNode(background);
    }
    
    function createFloatingParticle(container) {
        const particle = document.createElement('div');
        particle.className = 'floating-particle';
        
        // Random position
        particle.style.left = `${Math.random() * 100}%`;
        particle.style.top = `${Math.random() * 100}%`;
        
        // Random size
        const size = Math.random() * 4 + 2;
        particle.style.width = `${size}px`;
        particle.style.height = `${size}px`;
        
        // Random animation delay
        particle.style.animationDelay = `${Math.random() * 20}s`;
        
        container.appendChild(particle);
    }
    
    function createDataStream(container) {
        const stream = document.createElement('div');
        stream.className = 'data-stream';
        
        // Random position
        stream.style.left = `${Math.random() * 100}%`;
        stream.style.top = `${Math.random() * 50}%`;
        
        // Random height
        stream.style.height = `${Math.random() * 150 + 50}px`;
        
        // Random animation properties
        stream.style.setProperty('--translate-x', `${(Math.random() * 200 - 100)}px`);
        stream.style.setProperty('--rotate', `${Math.random() * 30 - 15}deg`);
        stream.style.animationDuration = `${Math.random() * 8 + 4}s`;
        stream.style.animationDelay = `${Math.random() * 5}s`;
        
        container.appendChild(stream);
    }
    
    function createTechNode(container) {
        const node = document.createElement('div');
        node.className = 'tech-node';
        
        // Random position
        node.style.left = `${Math.random() * 100}%`;
        node.style.top = `${Math.random() * 100}%`;
        
        // Random animation delay
        node.style.animationDelay = `${Math.random() * 4}s`;
        
        container.appendChild(node);
    }
    
    // Replace data streams and add new ones periodically
    setInterval(() => {
        const oldStreams = document.querySelectorAll('.data-stream');
        if (oldStreams.length > 0) {
            const randomIndex = Math.floor(Math.random() * oldStreams.length);
            oldStreams[randomIndex].remove();
            createDataStream(background);
        }
    }, 2000);
});



async function logout() {
    try {
        const response = await fetch('/logout', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            credentials: 'include'
        });

        if (response.ok) {
            window.location.href = '/login';
        } else {
            throw new Error('Logout failed');
        }
    } catch (error) {
        console.error('Logout error:', error);
        alert('Failed to logout. Please try again.');
    }
}



